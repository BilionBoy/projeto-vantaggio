<div class="container">
  <%= render "shared/notice" %>
  <div class="card border-0 shadow shadow-sm">
    <div class="card-header border-0 d-flex justify-content-between">
      <span class="fs-4">Visualizar P Prestador</span>
    </div>
    <div class="card-body">
      <div id="<%= dom_id @p_prestador %>">
          <p>
            <strong>Empresa Nome:</strong>
            <%= @p_prestador.empresa_nome %>
          </p>
          <p>
            <strong>Cnpj:</strong>
            <%= @p_prestador.cnpj %>
          </p>
          <p>
            <strong>P Categoria:</strong>
            <%= @p_prestador.p_categoria %>
          </p>
          <p>
            <strong>G Tipo Usuario:</strong>
            <%= @p_prestador.g_tipo_usuario %>
          </p>
          <p>
            <strong>G Status Usuario:</strong>
            <%= @p_prestador.g_status_usuario %>
          </p>
      </div>
    </div>
    <div class="card-footer border-0 d-flex justify-content-between">
      <div>
        <%= link_to edit_p_prestador_path(@p_prestador), class: "btn btn-primary" do %>
        <i class="bi bi-pencil-square"></i> Editar
        <% end %>
        <%= link_to "Voltar", p_prestadores_path, class: "btn btn-secondary" %>
      </div>
      <%= button_to "Deletar", @p_prestador, method: :delete, class: "btn btn-outline-danger border-0" %>
    </div>
  </div>
</div>
